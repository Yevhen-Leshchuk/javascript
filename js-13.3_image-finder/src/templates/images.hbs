{{#each this}}
 <li class="card">
  <div class="photo-card">
  <img class="photo-card__image"src="{{webformatURL}}" data-source="{{largeImageURL}}" alt="{{tags}}" />
  
    <div class="stats">
      <p class="stats-item">
        <i class="material-icons">thumb_up</i>
        {{likes}}
      </p>
      <p class="stats-item">
        <i class="material-icons">visibility</i>
        {{views}}
      </p>
      <p class="stats-item">
        <i class="material-icons">comment</i>
        {{comments}}
        129
      </p>
      <p class="stats-item">
        <i class="material-icons">cloud_download</i>
        {{downloads}}
      </p>
    </div>
  </div>
</li>
{{/each}}